{
    "name": "wolfram-alpha-mcp-server",
    "version": "1.0.0",
    "description": "MCP server for Wolfram Alpha LLM API integration",
    "main": "build/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "start": "node build/index.js"
    },
    "keywords": [
        "mcp",
        "wolfram",
        "wolfram-alpha",
        "mathematics",
        "computation",
        "knowledge",
        "api"
    ],
    "author": "MCP Community",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^0.5.0",
        "axios": "^1.6.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "mcp": {
        "serverName": "wolfram-alpha",
        "displayName": "Wolfram Alpha",
        "description": "Access Wolfram Alpha's computational knowledge engine through natural language queries",
        "category": "knowledge",
        "tools": [
            {
                "name": "wolfram_query",
                "description": "Query Wolfram Alpha for computational, mathematical, and factual information"
            }
        ]
    }
}